  <!-- JavaScript -->
  <script>
    const cart = {};

    function addToCart(productName) {
      const cartList = document.getElementById('cart-items');

      if (cart[productName]) {
        cart[productName]++;
      } else {
        cart[productName] = 1;
      }

      cartList.innerHTML = '';

      for (let item in cart) {
        const li = document.createElement('li');
        li.textContent = `${item} × ${cart[item]}`;
        cartList.appendChild(li);
      }
    }
  </script>

